{
  "name": "code-to-ui-mapper-vscode-extension",
  "displayName": "Code-to-UI-Mapper",
  "description": "VS Code extension to find React components in the browser UI.",
  "version": "0.1.2",
  "private": false,
  "publisher": "LucidLayer",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucidlayer/traceform.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-to-ui-mapper.findInUI",
        "title": "Code-to-UI: Find Component in UI"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == javascript || resourceLangId == typescript || resourceLangId == javascriptreact || resourceLangId == typescriptreact",
          "command": "code-to-ui-mapper.findInUI",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "format": "prettier --write src/**/*.ts",
    "test": "vscode-test"
  },
  "dependencies": {
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/vscode": "^1.80.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^3.3.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
